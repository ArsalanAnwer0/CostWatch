# CostWatch Frontend Environment Variables

# API Gateway URL (for production deployment)
REACT_APP_API_URL=http://localhost:8002

# Direct service URLs (for development/testing)
REACT_APP_RESOURCE_SCANNER_URL=http://localhost:8000
REACT_APP_COST_ANALYZER_URL=http://localhost:8001
REACT_APP_ANALYTICS_ENGINE_URL=http://localhost:8003
REACT_APP_ALERT_MANAGER_URL=http://localhost:8004

# AWS Account ID (optional, for demo/testing)
REACT_APP_AWS_ACCOUNT_ID=000000000000

# Feature flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ALERTS=true
REACT_APP_ENABLE_PREDICTIONS=true

# Environment
REACT_APP_ENVIRONMENT=development
