{
    "dashboard": {
      "id": null,
      "title": "CostWatch Overview",
      "description": "Main dashboard for CostWatch application monitoring",
      "tags": ["costwatch", "monitoring", "aws", "costs"],
      "timezone": "browser",
      "panels": [
        {
          "id": 1,
          "title": "Service Health Status",
          "type": "stat",
          "targets": [
            {
              "expr": "up{job=~\"api-gateway|cost-analyzer|resource-scanner|analytics-engine|alert-manager-service\"}",
              "legendFormat": "{{job}}"
            }
          ],
          "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
        },
        {
          "id": 2, 
          "title": "Request Rate",
          "type": "graph",
          "targets": [
            {
              "expr": "rate(http_requests_total[5m])",
              "legendFormat": "{{job}}"
            }
          ],
          "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
        },
        {
          "id": 3,
          "title": "Response Time",
          "type": "graph", 
          "targets": [
            {
              "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
              "legendFormat": "95th percentile"
            }
          ],
          "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
        },
        {
          "id": 4,
          "title": "Error Rate",
          "type": "graph",
          "targets": [
            {
              "expr": "rate(http_requests_total{status=~\"5..\"}[5m])",
              "legendFormat": "5xx errors"
            }
          ],
          "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
        }
      ],
      "time": {
        "from": "now-6h",
        "to": "now"
      },
      "refresh": "30s"
    }
  }